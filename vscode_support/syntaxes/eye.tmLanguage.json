{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Eye",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#constants"
    },
    {
      "include": "#func"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.eye",
          "match": "\\b(print|return|set|if|else|do|throw)\\b"
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.numeric.eye",
          "match": "\\b[-]?\\d+\\b"
        },
        {
          "name": "constant.numeric.eye",
          "match": "\\b(true|false)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.eye",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.eye",
          "match": "\\\\."
        }
      ]
    },
    "comments": {
      "name": "comment.line.eye",
      "begin": "//",
      "end": "\n"
    },
    "func": {
      "patterns": [
        {
          "name": "keyword.control.eye",
          "match": "proc"
        },
        {
          "name": "variable.name.eye",
          "begin": "\\bproc\\b",
          "end": "\\b(\\b",
          "match": "\\b[A-z][A-z0-9_]*\\b"
        }
      ]
    }
  },
  "scopeName": "source.eye"
}
